{
    "show_device": {
        "command": "show device",
        "description": "Show information from connected devices.",
        "usage": "show device",
        "demo": "show device"
    },
    "show_target": {
        "command": "show target",
        "description": "Show information from target app in application's workspace.",
        "usage": "show target (optional: --target <target_id|target_idx>,<target_id|target_idx>,..)",
        "demo": "• show target\n• show target --target injuredandroid\n• show target --target test,base"
    },
    "show_module_options": {
        "command": "show module options",
        "description": "Show options for selected exploit module.",
        "usage": "show module options",
        "demo": "show module options"
    },
    "decompile": {
        "command": "decompile",
        "description": "Decompile target apk to workspace.",
        "usage": "decompile <path_to_apk> <path_to_apk>.. (optional: --file <apk_list> --output <output_directory> --decompiler <decompiler (default: jadx, option: jadx, cfr)>)",
        "demo": "• decompile /home/user/path/to/target.apk\n• decompile --file /home/User/targets.txt --output /home/User/workspace --decompiler cfr"
    },
    "analyze_manifest": {
        "command": "analyze manifest",
        "description": "Analyze target manifest from workspace.",
        "usage": "analyze manifest (optional: --target [<target_id|target_idx>,<target_id|target_idx>,..][start_idx-end_idx] --json --output <output_file>)",
        "demo": "• analyze manifest\n• analyze manifest --target base\n• analyze manifest --target injuredandroid --json\n• analyze manifest --target insecureshop --json --output insecureshop.json"
    },
    "analyze_code": {
        "command": "analyze code",
        "description": "Analyze target codebase from workspace.",
        "usage": "analyze code (optional: --target [<target_id|target_idx>,<target_id|target_idx>,..][start_idx-end_idx] --json --output <output_file>)",
        "demo": "• analyze code\n• analyze code --target base\n• analyze code --target 1-10 --json\n• analyze code --target insecureshop --json --output insecureshop.json"
    },
    "analyze_*": {
        "command": "analyze *",
        "description": "Analyze target's both manifest and codebase from workspace.",
        "usage": "analyze * (optional: --target [<target_id|target_idx>,<target_id|target_idx>,..][start_idx-end_idx] --json --output <output_file>)",
        "demo": "• analyze *\n• analyze * --target base\n• analyze * --target injuredandroid --json\n• analyze * --target insecureshop --json --output insecureshop.json"
    },
    "use_device": {
        "command": "use device",
        "description": "Use a device connected to host.",
        "usage": "use device <device_id|device_idx>",
        "demo": "use device emulator-554"
    },
    "use_module": {
        "command": "use module",
        "description": "Use an exploit module.",
        "usage": "use module <module_name|module_idx>",
        "demo": "use module android/app/privilege-escalation/insecure-set-result/basic"
    },
    "remove_target": {
        "command": "remove target",
        "description": "Remove target from workspace.",
        "usage": "remove target (optional: --target <target_id|target_idx>,<target_id|target_idx>,..)",
        "demo": "• remove target\n• remove target --target injuredandroid\n• remove target --target 1,5"
    },
    "search_module": {
        "command": "search module",
        "description": "Search for exploit modules.",
        "usage": "search module <keyword>",
        "demo": "• search module *\n• search module android"
    },
    "set": {
        "command": "set",
        "description": "Set option value for selected exploit module.",
        "usage": "set <option>=<value> <option>=<value>...",
        "demo": "set TARGET_PACKAGE=com.test.test"
    },
    "execute": {
        "command": "execute",
        "description": "Execute an exploit module.",
        "usage": "execute",
        "demo": "execute"
    },
    "print_option": {
        "command": "print option",
        "description": "Print option input template.",
        "usage": "print option",
        "demo": "print option"
    },
    "add_target": {
        "command": "add",
        "description": "Add new target to target DB.",
        "usage": "add <target_path> <target_path>...",
        "demo": "add /home/User/Labs/android/sherlock"
    }
}